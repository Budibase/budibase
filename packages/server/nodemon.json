{
  "watch": [
    "src",
    "../backend-core/src",
    "../pro/src",
    "../types/src",
    "../shared-core/src",
    "../string-templates/src",
    "../../.env"
  ],
  "ext": "js,ts,json,svelte,hbs",
  "ignore": [
    "**/*.spec.ts",
    "**/*.spec.js",
    "../*/dist/**/*",
    "client/**/*",
    "builder/**/*"
  ],
  "exec": "yarn build && node --enable-source-maps --no-node-snapshot ./dist/index.js"
}
