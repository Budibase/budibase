<html>
  <script nonce="{{ nonce }}">
    document.fonts.ready.then(() => {
      // Safari requires that the origin be configured
      window.parent.postMessage({ type: "docLoaded" }, "*"); 
    })
  </script>
  <head>
    <script nonce="{{ nonce }}">
      window["##BUDIBASE_APP_ID##"] = "{{appId}}"
      window["##BUDIBASE_APP_EMBEDDED##"] = "{{embedded}}"
    </script>
    {{{head}}}
    <style>{{{css}}}</style>
  </head>
  {{{body}}}
</html>
