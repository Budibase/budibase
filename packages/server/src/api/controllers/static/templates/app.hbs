<html>
  <script nonce="{{ nonce }}">
    document.fonts.ready.then(() => {
      window.parent.postMessage({ type: "docLoaded" });
    })
  </script>
  <head>
    {{{head}}}
    <style>{{{css}}}</style>
  </head>

  <script nonce="{{ nonce }}">
    window["##BUDIBASE_APP_ID##"] = "{{appId}}"
    window["##BUDIBASE_APP_EMBEDDED##"] = "{{embedded}}"
  </script>

  {{{body}}}
</html>
