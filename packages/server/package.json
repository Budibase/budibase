{
  "name": "@budibase/server",
  "version": "0.0.32",
  "description": "Budibase Web Server",
  "main": "src/electron.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Budibase/budibase.git"
  },
  "build": {
    "appId": "com.budibase.builder",
    "productName": "Budibase Builder"
  },
  "scripts": {
    "test": "jest",
    "test:integration": "jest routes --runInBand",
    "test:watch": "jest -w",
    "initialise": "node ../cli/bin/budi init -b local -q",
    "budi": "node ../cli/bin/budi",
    "dev:builder": "nodemon ../cli/bin/budi run",
    "electron": "electron src/electron.js",
    "build:electron": "electron-builder",
    "build:electron:all": "electron-builder -mwl",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --fix src/"
  },
  "keywords": [
    "budibase"
  ],
  "author": "Michael Shanks",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@budibase/client": "^0.0.32",
    "@budibase/core": "^0.0.32",
    "@koa/router": "^8.0.0",
    "ajv": "^6.12.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.2.0",
    "fs-extra": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-compress": "^4.0.1",
    "koa-logger": "^3.2.1",
    "koa-send": "^5.0.0",
    "koa-session": "^5.12.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.13",
    "pouchdb": "^7.2.1",
    "pouchdb-all-dbs": "^1.0.2",
    "squirrelly": "^7.5.0",
    "tar-fs": "^2.0.0",
    "uuid": "^3.3.2",
    "yargs": "^13.2.4",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^24.8.0",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-updater": "^4.3.1",
    "eslint": "^6.8.0",
    "jest": "^24.8.0",
    "nodemon": "^2.0.2",
    "server-destroy": "^1.0.1",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "gitHead": "b1f4f90927d9e494e513220ef060af28d2d42455"
}
