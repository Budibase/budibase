{
  "name": "@budibase/frontend-core",
  "version": "0.0.0",
  "description": "Budibase frontend core libraries used in builder and client",
  "author": "Budibase",
  "license": "MPL-2.0",
  "type": "module",
  "exports": {
    ".": {
