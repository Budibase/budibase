{
  "include": ["src/**/*"],
  "compilerOptions": {
    "declaration": true,
    "target": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "incremental": true,
    "lib": ["dom"],
    "outDir": "dist",
    "esModuleInterop": true,
    "types": ["node", "jest"],
    "resolveJsonModule": true
  }
}
