    "lib/**/*.js"
  ],
  "homepage": "https://github.com/ladjs/superagent",
  "jsdelivr": "dist/superagent.min.js",
  "keywords": [
    "agent",
    "ajax",
    "ajax",
    "api",
    "async",
    "await",
