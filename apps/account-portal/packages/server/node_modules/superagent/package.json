    "browserify": "^17.0.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-xo-lass": "2",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-node": "^11.1.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "fixpack": "^4.0.0",
    "get-port": "4.2.0",
    "husky": "7",
    "lint-staged": "12",
